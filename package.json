    "element.scrollintoviewifneeded-polyfill": "^1.0.1",
    "firebase": "^10.12.0",
    "lodash": "^4.17.21",
    "preact": "^10.22.0",
    "preact-iso": "^2.6.2",
    "react-colorful": "^5.6.1",
    "react-easy-crop": "^5.0.7",
