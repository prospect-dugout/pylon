    "./style/mixins": "./dist/style/mixins.js",
    "./style/*": "./dist/style/*.css",
    "./configs/*": "./dist/configs/*"
  },
  "scripts": {
    "dev": "npm run storybook",
    "storybook": "storybook dev -p 6006",
